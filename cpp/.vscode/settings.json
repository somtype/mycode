    "C_Cpp.default.cppStandard": "c++17",
    "vim.useCtrlKeys": false,
    "files.associations": {
        "vector": "cpp",
        "algorithm": "cpp",
        "string_view": "cpp",
        "cstring": "cpp",
        "ostream": "cpp",
        "iostream": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "initializer_list": "cpp",
        "regex": "cpp",
        "utility": "cpp",
        "valarray": "cpp",
        "iosfwd": "cpp"
    }
}